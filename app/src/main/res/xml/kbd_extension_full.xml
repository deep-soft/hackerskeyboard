<Keyboard xmlns:hackkbd="http://schemas.android.com/apk/res-auto"
    hackkbd:keyWidth="7.1428571485%p"
    hackkbd:horizontalGap="0px"
    hackkbd:verticalGap="0px"
    >

    <Row hackkbd:rowEdgeFlags="top|bottom">
        <Key hackkbd:keyLabel="&#x300;" hackkbd:keyEdgeFlags="left"/>
        <Key hackkbd:keyLabel="&#x301;"/>
        <Key hackkbd:keyLabel="&#x302;"/>        
        <Key hackkbd:keyLabel="&#x303;"/>
        <Key hackkbd:keyLabel="&#x304;"/>
        <Key hackkbd:keyLabel="&#x305;"/>
        <Key hackkbd:keyLabel="&#x306;"/>
        <Key hackkbd:keyLabel="&#x307;"/>
        <Key hackkbd:keyLabel="&#x308;"/>
        <Key hackkbd:keyLabel="&#x30A;"/>
        <Key hackkbd:keyLabel="&#x30B;"/>
        <Key hackkbd:keyLabel="&#x30C;"/>
	<Key
            hackkbd:codes="@integer/key_home"
            hackkbd:isModifier="true"            
            hackkbd:keyLabel="Home" />
	<Key
            hackkbd:codes="@integer/key_end"
            hackkbd:isModifier="true"            
            hackkbd:keyLabel="End"
            hackkbd:keyEdgeFlags="right" />
        />
    </Row>
</Keyboard>